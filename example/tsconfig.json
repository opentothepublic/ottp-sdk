{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "strict": true,
    "skipLibCheck": true,
    "removeComments": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "useUnknownInCatchVariables": false,
    "forceConsistentCasingInFileNames": true,
  },
  "include": ["../test/src/**/*"],
  "exclude": ["node_modules"]
}
